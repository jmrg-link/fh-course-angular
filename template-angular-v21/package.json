{
  "name": "template-angular-v21",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/common": "21.1.3",
    "@angular/compiler": "21.1.3",
    "@angular/core": "21.1.3",
    "@angular/forms": "21.1.3",
    "@angular/platform-browser": "21.1.3",
    "@angular/router": "21.1.3",
    "rxjs": "~7.8.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "21.1.3",
    "@angular/cli": "21.1.3",
    "@angular/compiler-cli": "21.1.3",
    "angular-eslint": "21.2.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "jsdom": "28.0.0",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  }
}
